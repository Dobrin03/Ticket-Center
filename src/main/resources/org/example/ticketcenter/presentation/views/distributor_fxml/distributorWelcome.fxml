<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ticketcenter.controllers.distributors_controllers.DistributorWelcomeController">
   <children>
      <Label fx:id="lbl_welcome" alignment="CENTER" layoutX="144.0" layoutY="41.0" prefHeight="54.0" prefWidth="312.0" text="Welcome">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button alignment="CENTER" layoutX="215.0" layoutY="145.0" mnemonicParsing="false" onAction="#onCheckRequestsClick" prefHeight="54.0" prefWidth="154.0" text="Check Requests">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="483.0" layoutY="332.0" mnemonicParsing="false" onAction="#onLogOutClick" prefHeight="42.0" prefWidth="84.0" text="Log Out">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="215.0" layoutY="207.0" mnemonicParsing="false" onAction="#onCheckRatingClick" prefHeight="54.0" prefWidth="154.0" text="Check Rating">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="215.0" layoutY="277.0" mnemonicParsing="false" onAction="#onBoughtTicketsClick" prefHeight="54.0" prefWidth="154.0" text="Bought Tickets">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
