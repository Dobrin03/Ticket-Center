      <Label layoutX="74.0" layoutY="68.0" prefHeight="18.0" prefWidth="76.0" text="Choose Event" />
      <ComboBox fx:id="status_cb" layoutX="179.0" layoutY="288.0" prefHeight="26.0" prefWidth="380.0" />
